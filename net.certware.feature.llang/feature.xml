      <import plugin="org.eclipse.xtext.common.types.ui"/>
      <import plugin="org.eclipse.xtext.ui.codetemplates.ui"/>
      <import plugin="org.eclipse.compare"/>
      <import plugin="org.eclipse.ui.console" version="3.6.100" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.xtend"/>
      <import plugin="net.certware.argument.language.ui" version="1.0.0" match="greaterOrEqual"/>
